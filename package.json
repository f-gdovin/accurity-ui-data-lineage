{
  "name": "accurity-ui-data-lineage",
  "version": "1.0.0-SNAPSHOT",
  "description": "Data Lineage visualization tool for Glossary's Mappings",
  "repository": {
    "type": "git",
    "url": "https://git.simplity.eu/gdovinf/accurity-ui-data-lineage.git"
  },
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server -p --config webpack-dev-server.config.js --progress --inline --colors --hot",
    "build": "./node_modules/.bin/webpack -p --config webpack-production.config.js --progress",
    "build-no-progress": "./node_modules/.bin/webpack -p --config webpack-production.config.js",
    "lint": "./node_modules/.bin/eslint src && echo \"eslint: no lint errors\"",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --recursive tests",
    "test-watch": "./node_modules/.bin/mocha tests --recursive --watch --compilers js:babel-core/register",
    "test-coverage": "./node_modules/.bin/babel-istanbul cover --include-all-sources --root ./src/app ./node_modules/mocha/bin/_mocha -- tests --recursive --compilers js:babel-core/register"
  },
  "private": true,
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-eslint": "6.1.2",
    "babel-istanbul": "0.11.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "chai": "3.5.0",
    "eslint": "2.13.1",
    "eslint-plugin-react": "4.3.0",
    "html-webpack-plugin": "2.22.0",
    "mocha": "3.1.0",
    "react-hot-loader": "1.3.0",
    "transfer-webpack-plugin": "0.1.4",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "react-addons-test-utils": "15.3.0"
  },
  "dependencies": {
    "d3": "3.5.16",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "underscore": "^1.8.3"
  }
}
